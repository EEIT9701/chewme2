<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.iii.eeit9703.club.model.CommentVO" table="clubs">

		<id name="clubId" column="clubId">
			<generator class="identity" />
		</id>

		<property name="issueId" column="issueId" />
		<property name="clubId" column="clubId" />
		<property name="issueContent" column="issueContent" />
		<property name="proposerId" column="proposerId" />
		<property name="issuepic" column="issuepic" />
		<property name="comments" column="comments" />
	</class>
</hibernate-mapping>

