<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.iii.eeit9703.club.model.CommentVO" table="clubs">

		<id name="clubId" column="clubId">
			<generator class="identity" />
		</id>

		<property name="reqlyId" column="reqlyId" />
		<property name="qId" column="qId" />
		<property name="rContent" column="content" />
		<property name="replyerId" column="replyerId" />
		<property name="replyDate" column="replyDate" />
		<property name="read" column="read" />
	</class>
</hibernate-mapping>

